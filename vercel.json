{
  "version": 2,
  "rewrites": [
    { "source": "/dashboard/:path*", "destination": "/dashboard/:path*" },
    { "source": "/tutorials/:path*", "destination": "/dashboard/tutorials/:path*" },
    { "source": "/tutorials", "destination": "/dashboard/tutorials/index.html" },
    { "source": "/login", "destination": "/dashboard/login.html" },
    { "source": "/pricing", "destination": "/dashboard/pricing.html" },
    { "source": "/about", "destination": "/dashboard/about.html" },
    { "source": "/docs", "destination": "/dashboard/docs.html" },
    { "source": "/privacy", "destination": "/dashboard/privacy.html" },
    { "source": "/terms", "destination": "/dashboard/terms.html" },
    { "source": "/features", "destination": "/dashboard/features.html" },
    { "source": "/run", "destination": "/dashboard/run.html" },
    { "source": "/admin", "destination": "/dashboard/admin.html" },
    { "source": "/ai", "destination": "/dashboard/ai.html" },
    { "source": "/ai-api", "destination": "/dashboard/ai-api.html" },
    { "source": "/mcp", "destination": "/dashboard/mcp.html" },
    { "source": "/404", "destination": "/dashboard/404.html" },
    { "source": "/:path*", "destination": "/site/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}

